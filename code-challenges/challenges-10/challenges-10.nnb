                "#####  [Compare The Triplets](https://www.hackerrank.com/challenges/compare-the-triplets/problem?isFullScreen=true)\n\nSolution:"
            ],
            "outputs": [],
            "solution":{
                function compareTriplets(a, b) {
                let alice = 0;
                let bob =0 ;
                for(let i = 0 ; i< 3; i++){
                    if(a[i] > b[i]){
                        alice += 1
                    }else if (a[i] < b[i]){
                        bob += 1
                    }
                }
                return [alice,bob]
                }
            }
